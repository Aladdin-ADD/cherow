{
  "name": "@cherow/cherow",
  "version": "1.6.8",
  "description": "Fast and lightweight, standard-compliant javascript parser written in ECMAScript",
  "main": "dist/umd/cherow.js",
  "module": "dist/es2015/cherow.js",
  "jsnext:main": "dist/es2015/cherow.js",
  "types": "dist/types/cherow.d.ts",
  "typings": "dist/types/cherow.d.ts",
  "author": "KFlash",
  "contributors": [
    "Fred Kleuver <https://github.com/fkleuver>"
  ],
  "license": "ISC",
  "homepage": "https://github.com/cherow/cherow",
  "repository": {
    "type": "git",
    "url": "https://github.com/cherow/cherow"
  },
  "bugs": {
    "url": "https://github.com/cherow/cherow/issues"
  },
  "keywords": [
    "parsing",
    "ecmascript",
    "typescript",
    "parser",
    "performance",
    "tolerant",
    "estree",
    "es2018",
    "es2019",
    "ECMAScript 2019",
    "esnext",
    "javascript",
    "ast",
    "jsx",
    "tolerant",
    "lightweight"
  ],
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "cross-env tslint --project tsconfig-test.json -e \"test/**\" -t stylish",
    "build": "tsc -b",
    "dev": "tsc -b -w",
    "minify": "nps build.minify",
    "test": "nps test",
    "test:watch": "nps test.watch",
    "coverage": "nps coverage",
    "post_coverage": "nps coverage.post",
    "fuzz": "eslump ./test/fuzz/cherow.js .fuzz-output/"
  },
  "devDependencies": {
    "clean-assert": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.67.4",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2",
    "unexpected": "^10.39.2",
    "unicode-11.0.0": "^0.7.8"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "build/test/**/*.*"
    ]
  }
}
